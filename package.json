    "start-prod": "cd server && npm run production",
    "start": "node index.js",
    "ical-server": "nodemon index.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.1",
    "path": "^0.12.7",
    "react-slick": "^0.30.3",
    "serve": "^14.2.4",
    "slick-carousel": "^1.8.1"
  }
}
